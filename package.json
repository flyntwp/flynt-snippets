{
  "name": "flynt-vscode-snippets",
  "displayName": "Flynt Snippets",
  "description": "Snippets for development with the Flynt framework.",
  "version": "1.1.0",
  "publisher": "flyntwp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flyntwp/flynt-vscode-snippets/issues"
  },
  "homepage": "https://github.com/flyntwp/flynt-vscode-snippets/",
  "repository": {
    "type": "git",
    "url": "https://github.com/flyntwp/flynt-vscode-snippets/"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Flynt"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./snippets/json.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": "twig",
        "path": "./snippets/twig.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "release": "standard-version -a",
    "releaseGithub": "conventional-github-releaser -p angular"
  },
  "devDependencies": {
    "conventional-github-releaser": "^3.1.2",
    "standard-version": "^5.0.2"
  }
}
